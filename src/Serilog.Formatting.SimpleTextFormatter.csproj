<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <ImplicitUsings>disable</ImplicitUsings>
    <Nullable>disable</Nullable>
    <RootNamespace>Serilog.Formatting.SimpleTextFormatter</RootNamespace>
    <PackageId>Serilog.Formatting.SimpleTextFormatter</PackageId>
    <Authors>Daniel Buchanan</Authors>
    <Owners>Daniel Buchanan</Owners>
    <ProjectUrl>https://github.com/daniel-buchanan/Serilog.Formatting.SimpleTextFormatter</ProjectUrl>
    <IconUrl></IconUrl>
    <Tags>Serilog format simple</Tags>
    <PackageReadmeFile>README.md</PackageReadmeFile>
